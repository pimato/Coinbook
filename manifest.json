{
  "manifest_version": 2,
  "name": "Coinbook - Wallet",
  "short_name": "Coinbook",
  "description": "Private and Secure Ethereum Wallet",
  "version": "1.0.0",
  "author": "Thomas Pulber",
  "browser_action": {
    "default_title": "Open a new tab to view your bookmarks.",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage  "
  ]
}
